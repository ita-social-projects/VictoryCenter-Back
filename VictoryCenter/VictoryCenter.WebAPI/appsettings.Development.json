{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CORS": {
    "AllowedOrigins": ["http://localhost:3000", "https://localhost:3000"],
    "AllowedHeaders": ["*"],
    "AllowedMethods": ["*"],
    "PreflightMaxAge": 1,
    "ExposedHeaders": ["*"]
  }
}
