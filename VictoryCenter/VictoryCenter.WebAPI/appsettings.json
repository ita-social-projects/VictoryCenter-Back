{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Audience": "VictoryCenter.Client",
    "Issuer": "VictoryCenter.Api",
    "LifetimeInMinutes": 1440,
    "SecretKey": "${JWTOPTIONS_SECRETKEY}",
    "RefreshTokenSecretKey": "${JWTOPTIONS_REFRESH_TOKEN_SECRETKEY}",
    "RefreshTokenLifetimeInDays": 7
  },
  "CORS": {
    "AllowedOrigins": ["https://historycode.online"],
    "AllowedHeaders": ["*"],
    "AllowedMethods": ["*"],
    "ExposedHeaders": ["*"],
    "PreflightMaxAge": "600"
  }
}
