{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "BlobEnvironmentVariables": {
    "ServiceType": "Local",
    "Local": {
      "BlobStoreKey": "my-local-key",
      "BlobStorePath": "StaticFile/"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
