{
  "ConnectionStrings": {
    "DefaultConnection": "${INTEGRATION_TESTS_DB_CONNECTION_STRING}"
  },
  "BlobEnvironmentVariables": {
    "ServiceType": "Local",
    "Local": {
      "BlobStoreKey": "${BLOB_LOCAL_STORE_KEY}",
      "BlobStorePath": "StaticFile/"
    }
  },
  "JwtOptions": {
    "Audience": "VictoryCenter.Client",
    "Issuer": "VictoryCenter.Api",
    "LifetimeInMinutes": 1440,
    "SecretKey": "${JWTOPTIONS_SECRETKEY}",
    "RefreshTokenSecretKey": "${JWTOPTIONS_REFRESH_TOKEN_SECRETKEY}",
    "RefreshTokenLifetimeInDays": 7
  },
  "CORS": {
    "AllowedOrigins": ["*"],
    "AllowedHeaders": ["*"],
    "AllowedMethods": ["*"],
    "PreflightMaxAge": 1,
    "ExposedHeaders": ["*"]
  }
}

